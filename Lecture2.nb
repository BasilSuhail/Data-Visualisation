      "\n", "\t", 
      RowBox[{"dist", " ", "=", " ", 
       RowBox[{"2", " ", 
        RowBox[{"Max", "[", "r", "]"}]}]}], ";", "          ", 
      RowBox[{"(*", " ", 
       RowBox[{"distance", " ", "between", " ", "disks"}], " ", "*)"}], "\n", 
      "\t", "\n", "\t", 
      RowBox[{"Graphics", "[", "\n", "\t    ", 
       RowBox[{
        RowBox[{"Table", "[", 
